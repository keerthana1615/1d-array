import java.util.*;
public class Main {
    public static void main(String[] args) {
        int[] arr = {1,2,3,4,5};
        int n=arr.length;
        
        for(int i=0;i<n;i++){
            int sum=0;
        
        for(int j=i+1;j<n;j++){
            sum=sum+arr[j];
        }
           System.out.print(sum+" "); 
        }
   }
}
