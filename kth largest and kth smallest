import java.util.*;
public class Main {
    public static void main(String[] args) {
        int[] arr = {1,2,3,4,5,6,8};
        int k = 3;
        Arrays.sort(arr);
        int kthSmallest = arr[k - 1];
        int kthLargest = arr[arr.length - k];
        System.out.println("Kth Smallest: " + kthSmallest);
        System.out.println("Kth Largest: " + kthLargest);
    }
}
