import java.util.Scanner;
public class Main{
  public static void main(String[] args){  
 int arr[]={1,0,0,3,4,5};
int index = 0;
int n=arr.length;
for (int i = 0; i < n; i++) {
    if (arr[i] != 0) {
        arr[index++] = arr[i];
    }
}
while (index < n) {
    arr[index++] = 0;
}
for (int i : arr) {
    System.out.print(i + " ");
}
}
}
