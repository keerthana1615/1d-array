import java.util.*;
public class Main
{
	public static void main(String[] args) {
		Scanner s=new Scanner(System.in);
		int n=s.nextInt();
		int arr[]=new int[n];
		for(int i=0;i<n;i++){
		    arr[i]=s.nextInt();
		}
	   int min=1;
	   for(int i=0;i<n;i++){
	       if(min>arr[i])
	           min=arr[i];
	   }
		System.out.print("minimum: "+min);
	}
}
